            <Grid Opacity="1" HorizontalAlignment="Left" VerticalAlignment="Top" Background="#00000000">
              <TextBlock Opacity="0.6" x:Name="textBlock" Foreground="#FFFFFFFF" Text="{TemplateBinding Content}" TextWrapping="Wrap"/>
            </Grid>
            <ControlTemplate.Triggers>
              <Trigger Property="IsFocused" Value="True"/>
              <Trigger Property="IsDefaulted" Value="True"/>
              <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Opacity" TargetName="textBlock" Value="1"/>
              </Trigger>
              <Trigger Property="IsPressed" Value="True"/>
              <Trigger Property="IsEnabled" Value="False"/>
            </ControlTemplate.Triggers>
          </ControlTemplate>
        </Setter.Value>
      </Setter>
